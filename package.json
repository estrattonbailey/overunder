{
  "name": "overunder",
  "version": "0.4.1",
  "description": "A small waypoint library that emits events when you scroll to a specific element(s), or resize to a specified width.",
  "main": "dist/overunder.js",
  "scripts": {
    "build": "mkdir -p dist && browserify index.js --standalone overunder > dist/overunder.js",
    "size": "bundle-size ./dist/overunder.js",
    "test": "ava test -v"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/overunder.git"
  },
  "keywords": [
    "scroll",
    "waypoint",
    "resize",
    "sticky",
    "element queries"
  ],
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/overunder/issues"
  },
  "homepage": "https://github.com/estrattonbailey/overunder#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "bundle-size": "^0.7.0",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "minifyify": "^7.3.3",
    "query-string": "^4.2.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "knot.js": "^1.1.1"
  }
}
