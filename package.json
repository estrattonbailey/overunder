{
  "name": "overunder",
  "version": "0.0.5",
  "description": "Lets you know where you're at.",
  "main": "dist/overunder.js",
  "scripts": {
    "js:umd": "mkdir browser ; browserify index.js --standalone overunder > browser/overunder.js"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/overunder.git"
  },
  "keywords": [
    "scroll",
    "waypoint",
    "resize"
  ],
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/overunder/issues"
  },
  "homepage": "https://github.com/estrattonbailey/overunder#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "minifyify": "^7.3.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "knot.js": "^1.1.1"
  }
}
